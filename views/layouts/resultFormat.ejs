<section class="book-view container">
        <ul id=bookObject>
            <li class="book-view" data-id='<%=books.title %>'>
            <% books.forEach(book => { %>
                <h4>Title: <%= book.title %></h4>
                <img src= '<% book.img %>''>
                <p>Author: <%= book.authors%> </p>
                <p> ISBN 10: <%= book.isbn10 %> </p>
                <p> ISBN 13: <%= book.isbn13 %></p>
                <p>Description: <%= book.description %> </p>
           <% }) %>
              
            </li>
        </ul>
    </section>